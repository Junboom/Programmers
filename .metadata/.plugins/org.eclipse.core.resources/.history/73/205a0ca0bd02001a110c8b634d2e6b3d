        	if (s.charAt(i) != '{' && s.charAt(i) != '}' && s.charAt(i) != ',') {
				
			}
        	else {
        		sb = new StringBuilder();
        	}
		}
        return answer;
    }
	
	public static void main(String[] args) {
		System.out.println(Arrays.toString(solution("{{2},{2,1},{2,1,3},{2,1,3,4}}")));
