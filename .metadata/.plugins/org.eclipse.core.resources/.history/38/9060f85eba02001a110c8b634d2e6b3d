package kakao;

public class Solution_3 {

	public static class Node {
		Node[] next;
		int size;
		
		Node() {
			next = new Node[36];
			size = 0;
		}
	}
	
	public static int solution(String[] user_id, String[] banned_id) {
        int answer = 0;
        Node root = new Node();
        for (int i = 0; i < user_id.length; ++i) {
        	Node next = root;
			char[] ch = user_id[i].toCharArray();
			for (int j = 0; j < ch.length; ++j) {
				if ('0' <= ch[j] && ch[j] <= '9') {
					if (next.next[ch[j]-'0'] == null)
						next.next[ch[j]-'0'] = new Node();
					next = next.next[ch[j]-'0'];
				}
				else {
					if (next.next[10+ch[j]-'a'] == null)
						next.next[10+ch[j]-'a'] = new Node();
					next = next.next[10+ch[j]-'a'];
				}
			}
		}
        for (int i = 0; i < banned_id.length; i++) {
        	Node next = root;
			char[] ch = banned_id[i].toCharArray();
			for (int j = 0; j < ch.length; ++j) {
				if (ch[j] != '*') {
					if ('0' <= ch[j] && ch[j] <= '9') {
						if (next.next[ch[j]-'0'] == null)
							continue;
						next = next.next[ch[j]-'0'];
					}
					else {
						if (next.next[10+ch[j]-'a'] == null)
							continue;
						next = next.next[10+ch[j]-'a'];
					}
				}
			}
			++answer;
		}
        return answer;
    }
	
	public static void main(String[] args) {
		String[] user_id = {"frodo", "fradi", "crodo", "abc123", "frodoc"};
		String[] banned_id = {"fr*d*", "abc1**"};
		System.out.println(solution(user_id, banned_id));
	}

}
